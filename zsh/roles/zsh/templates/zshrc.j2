# Path to your oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh

ZSH_THEME="robbyrussell"

# Plugins
plugins=(
    git
    zsh-autosuggestions
    zsh-syntax-highlighting
)

# User configuration

source $ZSH/oh-my-zsh.sh
source ~/.profile

# Alias 
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

alias h='history'
alias hs='history | grep'
alias hsi='history | grep -i'

